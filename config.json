{
  "log": {
    "access": "",
    "error": "",
    "loglevel": "warning"
  },
  "policy": {
    "levels": {
      "0": {
        "handshake": 4,
        "connIdle": 300,
        "uplinkOnly": 2,
        "downlinkOnly": 5,
        "statsUserUplink": false,
        "statsUserDownlink": false,
        "bufferSize": 10240
      },
      "1": {
        "handshake": 4,
        "connIdle": 300,
        "uplinkOnly": 2,
        "downlinkOnly": 5,
        "statsUserUplink": true,
        "statsUserDownlink": true,
        "bufferSize": 10240
      }
    },
    "system": {
      "statsInboundUplink": false,
      "statsInboundDownlink": false
    }
  },
  "dns": {
    "servers": ["8.8.8.8", "8.8.4.4", "1.1.1.1", "1.0.0.1"]
  },
  "inbounds": [
    {
      "port": 28824,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "PLACEHOLDER_VMESS_WS_PASSWD",
            "level": 0,
            "alterId": 128,
            "security": "auto",
            "email": "vmess.ws@v2ray.com"
          }
        ],
        "disableInsecureEncryption": true
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "PLACEHOLDER_VMESS_WS_PATH"
        }
      },
      "tag": "vmwsin"
    },
    {
      "port": 28825,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "PLACEHOLDER_VMESS_H2_PASSWD",
            "level": 0,
            "alterId": 128,
            "security": "auto",
            "email": "vmess.h2@v2ray.com"
          }
        ],
        "disableInsecureEncryption": true
      },
      "streamSettings": {
        "network": "h2",
        "security": "none",
        "httpSettings": {
          "path": "PLACEHOLDER_VMESS_H2_PATH"
        }
      },
      "tag": "vmh2in"
    },
    {
      "port": 28827,
      "protocol": "shadowsocks",
      "settings": {
        "method": "aes-128-gcm",
        "ota": false,
        "password": "PLACEHOLDER_SS_WS_PASSWD",
        "network": "tcp"
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "PLACEHOLDER_SS_WS_PATH"
        }
      },
      "tag": "sswsin"
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "reject"
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "domain": ["geosite:cn", "geosite:category-ads-all"],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "domain": ["geosite:geolocation-!cn"],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "ip": ["geoip:cn", "geoip:private"],
        "outboundTag": "direct"
      }
    ]
  }
}
